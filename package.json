{
  "name": "restaurant-menu-chatbot",
  "version": "5.0.0",
  "description": "A multi-tenant restaurant menu chatbot SaaS with Next.js, PostgreSQL, and Perplexity AI",
  "type": "module",
  "scripts": {
    "start": "cd nextjs && npm run start",
    "dev": "cd nextjs && npm run dev",
    "build": "cd nextjs && npm run build",
    "nextjs:install": "cd nextjs && npm install",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "node prisma/seed.mjs",
    "db:migrate": "npx prisma migrate dev",
    "test": "echo \"No tests yet\"",
    "lint": "cd nextjs && npm run lint"
  },
  "keywords": [
    "nextjs",
    "chatbot",
    "restaurant",
    "menu",
    "perplexity",
    "multi-tenant",
    "saas",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "pg": "^8.18.0",
    "prisma": "^7.3.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "tsx": "^4.21.0"
  }
}
